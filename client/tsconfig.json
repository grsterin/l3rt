{
  "version": "0.0.0",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist"
  },
  "typeRoots": ["node_modules/@types"],
  "exclude": [
    "dist",
    "node_modules",
    "build"
  ]
}